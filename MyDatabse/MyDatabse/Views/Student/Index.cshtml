@model List<MyDatabse.Models.student>
@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Index</title>
</head>
<body>
    <h2>Index Page</h2>
    <div> 
        <table border="1" class="table-hover">
            <tr>
                <th>Id</th>
                <th>First Name</th>
                <th>Last Name</th>
                <th>Program</th>
                <th>Delete</th>
                <th>Edit</th>
            </tr>
            @foreach (var item in Model)
            {
                <tr>
                    <td>@item.Id</td>
                    <td>@item.fname</td>
                    <td>@item.lname</td>
                    <td>@item.Program</td>
                    <td>
                       @Html.ActionLink("Delete", "Delete" , new { id=item.Id })
                    </td>
                    <td>
                        @Html.ActionLink("Edit", "Edit", new { id = item.Id })
                    </td>
                </tr>
    
            }     
        </table>
       
    </div>
</body>
</html>
